<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vrushali.hospital">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="@xml/backup_descriptor"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Hospital">
        <activity
            android:name=".Direct"
            android:exported="true" />
        <activity
            android:name=".Adminlite"
            android:exported="true"
            android:parentActivityName=".Adminlogin1" />
        <activity
            android:name=".CategorySelected"
            android:exported="true" />
        <activity
            android:name=".Adminreg"
            android:exported="true"
            android:label="Registration"
            android:parentActivityName=".Adminlogin1" />
        <activity
            android:name=".MainActivitylite"
            android:exported="true"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Profile"
